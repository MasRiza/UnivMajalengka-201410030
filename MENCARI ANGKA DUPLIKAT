#include <iostream>
using namespace std;

int main(){
	    
    int arr[] = {2,8,9,6,1,3,8,6,7,8,9};     
          
    int length = sizeof(arr)/sizeof(arr[0]);  
    
    cout<<" MENCARI ANGKA DUPLIKAT "<<endl;    
    
    cout<<"Angka Duplikat Berada Pada Baris : ";     
    for(int i = 0; i < length; i++) {    
    for(int j = i + 1; j < length; j++) {    
        if(arr[i] == arr[j])    
            cout<<"\n" << arr[j];    
        }    
    }    
    return 0;    
}
